body{
	font-family: Tahoma;
	font-size: 11pt;
}
th{
	font-size: 13pt;
	color: 	#ff0000;
	padding: 5px;
}
table{
	border-collapse: collapse;
	border: 1px groove #000000;
}
td{
	padding: 5px;
}
input{
	padding: 5px;
	width: 200px;
	border: 1px dotted #dc143c;
	background:#00bfff;
}
button{
	border: 1px groove #000000;
	background:#ffd700;
	padding: 5px 15px 5px 15px;
	border-radius: 10px;
}